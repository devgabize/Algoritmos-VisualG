algoritmo "ConteAteUsuárioQuiser"
var
   valor, contador: Inteiro
inicio
      contador <- 0
      Escreva("quer contar até quanto? ")
      Leia(valor)
      Enquanto (contador <= valor) faca
               Escreval(contador)
               contador <- contador + 1
      FimEnquanto
      Escreva("Terminei de contar")
fimalgoritmo
